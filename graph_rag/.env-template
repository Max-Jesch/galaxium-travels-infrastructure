# Environment variables for Galaxium Graph RAG System
# Copy this file to .env and fill in your API keys

# OpenAI API Key (required for embeddings and LLM)
OPENAI_API_KEY=your_openai_api_key_here

# AstraDB Configuration (required for vector storage)
ASTRA_DB_API_ENDPOINT=your_astradb_endpoint_here
ASTRA_DB_APPLICATION_TOKEN=your_astradb_token_here
ASTRA_DB_KEYSPACE=your_keyspace_here

# Collection Configuration
ASTRA_DB_COLLECTION_NAME=galaxium_travels_documents_2

# Optional: LangSmith for tracing (if you want to use it)
# LANGCHAIN_API_KEY=your_langsmith_api_key_here
# LANGCHAIN_TRACING_V2=true